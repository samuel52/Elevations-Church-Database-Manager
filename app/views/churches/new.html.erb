<div class="content-page">
        <!-- Start content -->
        <div class="content">
          <div class="container">

            <!-- Page-Title -->
                        <div class="row">
                            <div class="col-sm-12">
                                <h4 class="page-title">Import CSV</h4>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-lg-12">
                                <div class="card-box">
                                  <%= form_tag import_churches_path, :class => 'form-inline', multipart: true do %>
                                  <div class="form-group">
                                  <%= file_field_tag :file, :class => 'form-control' %>
                              	  </div>
                              	  <div class="form-group">
                                  <%= submit_tag "Import Now", :class => 'btn btn-primary ' %>
                              	  </div>
                                  <% end -%>
                             	</div>
                            </div>
                        </div>   
           </div>
        </div>
</div>             

<%= render 'form', church: @church %>
